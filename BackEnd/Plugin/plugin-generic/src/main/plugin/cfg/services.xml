<Services>
	<Service name="FIX-Client">
        <className>com.exactpro.sf.services.fix.FIXClient</className>
        <settingsClassName>com.exactpro.sf.services.fix.FIXClientSettings</settingsClassName>
        <dictionaryValidatorFactoryName>com.exactpro.sf.configuration.dictionary.FIXDictionaryValidatorFactory
        </dictionaryValidatorFactoryName>
	</Service>
	<Service name="TCPIPClientService">
        <className>com.exactpro.sf.services.tcpip.TCPIPClient</className>
        <settingsClassName>com.exactpro.sf.services.tcpip.TCPIPSettings</settingsClassName>
        <messageHandlerClassName>com.exactpro.sf.services.tcpip.TCPIPMessageHandler</messageHandlerClassName>
	</Service>
    <Service name="ITCH-TCP-Client">
        <className>com.exactpro.sf.services.itch.ITCHTcpClient</className>
        <settingsClassName>com.exactpro.sf.services.itch.ITCHTCPClientSettings</settingsClassName>
        <dictionaryValidatorFactoryName>
            com.exactpro.sf.configuration.dictionary.ITCHDictionaryValidatorFactory
        </dictionaryValidatorFactoryName>
	</Service>
    <Service name="ITCH-UDP-Client">
        <className>com.exactpro.sf.services.itch.ITCHUdpClient</className>
        <settingsClassName>com.exactpro.sf.services.itch.ITCHClientSettings</settingsClassName>
        <dictionaryValidatorFactoryName>
            com.exactpro.sf.configuration.dictionary.ITCHDictionaryValidatorFactory
        </dictionaryValidatorFactoryName>
	</Service>
    <Service name="NTG-Client">
        <className>com.exactpro.sf.services.ntg.NTGClient</className>
        <settingsClassName>com.exactpro.sf.services.ntg.NTGClientSettings</settingsClassName>
        <dictionaryValidatorFactoryName>
            com.exactpro.sf.configuration.dictionary.NTGDictionaryValidatorFactory
        </dictionaryValidatorFactoryName>
	</Service>
</Services>
